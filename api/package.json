{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server.ts",
    "docker:up": "docker compose -f 'docker-compose.test.yml' up -d",
    "docker:down": "docker compose -f docker-compose.test.yml down",
    "migrate": "npx dotenv-cli -e .env.test.local -- npx prisma migrate deploy",
    "test": "NODE_ENV=testing npm run docker:up && npm run migrate && jest && npm run docker:down",
    "test:unit": "NODE_ENV=testing jest --testMatch '<rootDir>/src/**/*.unit.(spec|test).ts' --passWithNoTests",
    "test:int": "NODE_ENV=testing npm run docker:up && npm run migrate && jest --testMatch '<rootDir>/src/**/*.int.(spec|test).ts' --passWithNoTests && npm run docker:down"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "cloudinary": "^1.36.1",
    "connect-redis": "^7.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "helmet": "^6.1.5",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "redis": "^4.6.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/connect-redis": "^0.0.20",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.12",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.13.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
